@article{gertlerMicrowaveFilteringUsing2020,
  title = {Microwave Filtering Using Forward {{Brillouin}} Scattering in Photonic-Phononic Emit-Receive Devices},
  author = {Gertler, Shai and Kittlaus, Eric A. and Otterstrom, Nils T. and Kharel, Prashanta and Rakich, Peter T.},
