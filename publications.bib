@article{broengHighlyIncreasedPhotonic1998,
  title = {Highly Increased Photonic Band Gaps in Silica/Air Structures},
  author = {Broeng, Jes and Barkou, Stig E and Bjarklev, Anders and Knight, Jonathan C and Birks, Tim A and Russell, Philip St. J},
  year = {1998},
