  langid = {english},
  keywords = {Fiber optic couplers,High power lasers,Laser beams,Optical manipulation,Optical tweezers,Wavelength division multiplexers},
  file = {E:\OneDrive - University of Rochester\Graduate\Research\Paper\2021\Liu et al. - 2021 - Self-feedback induced bistability in dual-beam intracavity optical tweezers.pdf}
}

