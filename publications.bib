@article{safavi-naeiniControllingPhononsPhotons2019,
  title = {Controlling Phonons and Photons at the Wavelength Scale: Integrated Photonics Meets Integrated Phononics},
  shorttitle = {Controlling Phonons and Photons at the Wavelength Scale},
  author = {{Safavi-Naeini}, Amir H. and Thourhout, Dries Van and Baets, Roel and Laer, Rapha{\"e}l Van},
  year = {2019},
