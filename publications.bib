  author = {Kuang, Tengfang and Huang, Ran and Xiong, Wei and Zuo, Yunlan and Han, Xiang and Nori, Franco and Qiu, Cheng-Wei and Luo, Hui and Jing, Hui and Xiao, Guangzong},
  year = {2023},
