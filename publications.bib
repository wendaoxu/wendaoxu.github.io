  month = mar,
  journal = {Phys. Rev. Lett.},
  volume = {108},
  number = {9},
  pages = {093903},
  publisher = {American Physical Society},
  doi = {10.1103/PhysRevLett.108.093903},
  urldate = {2024-08-31},
  abstract = {It is shown that optomechanical forces can cause nonlinear self-channeling of light in a planar dual-slab waveguide. A system of two parallel silica nanowebs, spaced {$\sim$}100 nm and supported inside a fiber capillary, is studied theoretically and an iterative scheme developed to analyze its nonlinear optomechanical properties. Steady-state field distributions and mechanical deformation profiles are obtained, demonstrating that self-channeling is possible in realistic structures at launched powers as low as a few mW. The differential optical nonlinearity of the self-channeled mode can be as much as 10{\texttimes}106 times higher than the corresponding electronic Kerr nonlinearity. It is also intrinsically broadband, does not utilize resonant effects, can be viewed as a consequence of the extreme nonlocality of the mechanical response, and in fact is a notable example of a so-called accessible soliton.},
  file = {E\:\\OneDrive - University of Rochester\\Graduate\\Research\\Paper\\Phys. Rev. Lett.2012\\Butsch et al. - 2012 - Optomechanical Self-Channeling of Light in a Suspended Planar Dual-Nanoweb Waveguide.pdf;C\:\\Users\\panda\\Zotero\\storage\\I6ZYBR7B\\PhysRevLett.108.html}
}

