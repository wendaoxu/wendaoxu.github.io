@article{bashanForwardStimulatedBrillouin2021,
  title = {Forward Stimulated {{Brillouin}} Scattering and Opto-Mechanical Non-Reciprocity in Standard Polarization Maintaining Fibres},
  author = {Bashan, Gil and Diamandi, Hilel Hagai and London, Yosef and Sharma, Kavita and Shemer, Keren and Zehavi, Elad and Zadok, Avi},
  year = {2021},
