  number = {19},
  pages = {21235--21246},
  publisher = {Optica Publishing Group},
  issn = {1094-4087},
  doi = {10.1364/OE.20.021235},
  urldate = {2022-03-20},
  abstract = {We propose a scheme for on-chip isolation in chalcogenide (As2S3) rib waveguides, in which Stimulated Brillouin Scattering is used to induce non-reciprocal mode conversion within a multi-moded waveguide. The design exploits the idea that a chalcogenide rib buried in a silica matrix acts as waveguide for both light and sound, and can also be designed to be multi-moded for both optical and acoustic waves. The enhanced opto-acoustic coupling allows significant isolation (\&gt; 20 dB) within a chip-scale (cm-long) device (\&lt; 10 cm). We also show that the bandwidth of this device can be dramatically increased by tuning the dispersion of the waveguide to match the group velocity between optical modes: we find that 20 dB isolation can be extended over a bandwidth of 25 nm.},
  keywords = {Brillouin scattering,Mode conversion,Optical fields,Stimulated Brillouin scattering,Stimulated scattering,Waveguide modes},
  file = {E:\OneDrive - University of Rochester\Graduate\Research\Paper\Optics Express, Vol. 20, Issue 19, pp. 21235-212462012\Byrnes et al_2012_Design for broadband on-chip isolator using stimulated Brillouin scattering in.pdf}
}

