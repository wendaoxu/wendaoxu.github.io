@article{adamuDeepUVMidIRSupercontinuum2018,
  title = {Deep-{{UV}} to Mid-{{IR}} Supercontinuum Generation Driven by Mid-{{IR}} Ultrashort Pulses in a Gas-Filled Fiber},
  author = {Adamu, Abubakar I. and Habib, Md Selim and Petersen, Christian R. and {Antonio-Lopez}, J. Enrique and Zhou, Binbin and Sch{\"u}lzgen, Axel and {Amezcua-Correa}, Rodrigo and Bang, Ole and Markos, Christos},
  year = {2018},
  month = may,
  journal = {arXiv:1805.03118 [physics]},
  eprint = {1805.03118},
  primaryclass = {physics},
  urldate = {2019-03-03},
  abstract = {Supercontinuum (SC) generation based on ultrashort pulse compression constitutes one of the most promising technologies towards an ultra-wide bandwidth, high-brightness and spatially coherent light sources for applications such as spectroscopy and microscopy. Here, multi-octave SC generation in a gas-filled hollow-core antiresonant fiber (HC-ARF) is reported spanning from 200 nm in the deep ultraviolet (DUV) to 4000 nm in the mid-infrared (mid-IR). A measured average output power of 5 mW was obtained by pumping at the center wavelength of the first anti-resonance transmission window (2460 nm) with {\textasciitilde}100 fs pulses and an injected pulse energy of {\textasciitilde}7-8 \{{\textbackslash}mu\}J. The mechanism behind the extreme spectral broadening relies upon intense soliton-plasma nonlinear dynamics which leads to efficient soliton self-compression and phase-matched dispersive wave (DW) emission in the DUV region. The strongest DW is observed at 275 nm having an estimated pulse energy of 1.42 \{{\textbackslash}mu\}J, corresponding to 28.4 \% of the total output energy. Furthermore, the effect of changing the pump pulse energy and gas pressure on the nonlinear dynamics and their direct impact on SC generation was investigated. The current work paves a new way towards novel investigations of gas-based ultrafast nonlinear optics in the emerging mid-IR spectral regime.},
  archiveprefix = {arXiv},
  keywords = {Physics - Optics},
  file = {C\:\\Users\\panda\\Zotero\\storage\\YF5MS6P2\\Adamu et al_2018_Deep-UV to mid-IR supercontinuum generation driven by mid-IR ultrashort pulses.pdf;C\:\\Users\\panda\\Zotero\\storage\\FFEZYFNA\\1805.html}
}

