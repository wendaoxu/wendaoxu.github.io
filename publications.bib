@article{barlandCavitySolitonsPixels2002,
  title = {Cavity Solitons as Pixels in Semiconductor Microcavities},
  author = {Barland, Stephane and Tredicce, Jorge R. and Brambilla, Massimo and Lugiato, Luigi A. and Balle, Salvador and Giudici, Massimo and Maggipinto, Tommaso and Spinelli, Lorenzo and Tissoni, Giovanna and Kn{\"o}dl, Thomas and Miller, Michael and J{\"a}ger, Roland},
  year = {2002},
