@article{dongBrillouinscatteringinducedTransparencyNonreciprocal2015,
  title = {Brillouin-Scattering-Induced Transparency and Non-Reciprocal Light Storage},
  author = {Dong, Chun-Hua and Shen, Zhen and Zou, Chang-Ling and Zhang, Yan-Lei and Fu, Wei and Guo, Guang-Can},
  year = {2015},
